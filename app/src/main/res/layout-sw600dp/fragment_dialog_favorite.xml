<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:weightSum="100"
    android:background="@color/white"
    tools:context="com.sellcom.apps.tracker_material.Fragments.FragmentDialogHelp">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:weightSum="100"
        android:paddingTop="20dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_marginTop="20dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/fav_no_guia"
                        android:textSize="@dimen/sizeMediumText"
                        android:textStyle="bold"
                        android:textColor="@color/estafeta_text"/>
                    <TextView
                        android:id                  ="@+id/fav_no_guia"
                        android:layout_width        ="wrap_content"
                        android:layout_height       ="wrap_content"
                        android:textColor           ="@color/estafeta_text"
                        android:textSize            ="@dimen/sizeNormalText"
                        android:text=""/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/fav_cod_rastreo"
                        android:textSize="@dimen/sizeMediumText"
                        android:textStyle="bold"
                        android:textColor="@color/estafeta_text"/>
                    <TextView
                        android:id                  ="@+id/fav_cod_rastreo"
                        android:layout_width        ="wrap_content"
                        android:layout_height       ="wrap_content"
                        android:textColor           ="@color/estafeta_text"
                        android:textSize            ="@dimen/sizeNormalText"
                        android:text=""/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/fav_origen"
                        android:textSize="@dimen/sizeMediumText"
                        android:textStyle="bold"
                        android:textColor="@color/estafeta_text"/>
                    <TextView
                        android:id                  ="@+id/fav_origen"
                        android:layout_width        ="wrap_content"
                        android:layout_height       ="wrap_content"
                        android:textColor           ="@color/estafeta_text"
                        android:textSize            ="@dimen/sizeNormalText"
                        android:text=""/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:orientation="vertical"
                    android:layout_marginTop="5dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/fecha_recoleccion"
                        android:textColor="@color/estafeta_text"
                        android:textSize="@dimen/sizeMediumText"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/fd_fecha_recol"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" "
                        android:textColor="@color/estafeta_text"
                        android:textSize="@dimen/sizeNormalText" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_marginTop="20dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:textColor="@color/estafeta_text">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/estafeta_text"
                        android:text="@string/fav_destino"
                        android:textSize="@dimen/sizeMediumText"
                        android:textStyle="bold"/>
                    <TextView
                        android:id                  ="@+id/fav_destino"
                        android:layout_width        ="wrap_content"
                        android:layout_height       ="wrap_content"
                        android:textColor           ="@color/estafeta_text"
                        android:textSize            ="@dimen/sizeNormalText"
                        android:text=""/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/estafeta_text"
                        android:text="@string/fav_cp_destino"
                        android:textSize="@dimen/sizeMediumText"
                        android:textStyle="bold"/>
                    <TextView
                        android:id                  ="@+id/fav_cp_destino"
                        android:layout_width        ="wrap_content"
                        android:layout_height       ="wrap_content"
                        android:textColor           ="@color/estafeta_text"
                        android:textSize            ="@dimen/sizeNormalText"
                        android:text=""/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_marginTop="10dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/fav_estatus"
                        android:textSize="@dimen/sizeMediumText"
                        android:textStyle="bold"
                        android:textColor="@color/estafeta_text"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                    <TextView
                        android:id                  ="@+id/fav_estatus"
                        android:layout_width        ="0dp"
                        android:layout_height       ="wrap_content"
                        android:layout_weight="1"
                        android:textColor           ="@color/estafeta_text"
                        android:textSize            ="@dimen/sizeNormalText"
                        android:text=""/>
                    <ImageView
                        android:id="@+id/fd_img_status"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:layout_gravity="center"
                        android:scaleType="centerInside"
                        android:layout_weight="0"
                        android:src="@drawable/estatus_sin" />

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_marginTop="10dp">
                <LinearLayout
                    android:id="@+id/lin_entrega"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="5dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/fav_fecha"
                        android:textSize="@dimen/sizeMediumText"
                        android:textStyle="bold"
                        android:textColor="@color/estafeta_text"/>
                    <TextView
                        android:id                  ="@+id/fav_fecha"
                        android:layout_width        ="wrap_content"
                        android:layout_height       ="wrap_content"
                        android:textColor           ="@color/estafeta_text"
                        android:textSize            ="@dimen/sizeNormalText"
                        android:text=""/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/lin_recibio"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/fav_recibio"
                        android:textSize="@dimen/sizeMediumText"
                        android:textStyle="bold"
                        android:textColor="@color/estafeta_text"/>
                    <TextView
                        android:id                  ="@+id/fav_recibio"
                        android:layout_width        ="wrap_content"
                        android:layout_height       ="wrap_content"
                        android:textColor           ="@color/estafeta_text"
                        android:textSize            ="@dimen/sizeNormalText"
                        android:text=""/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_marginBottom="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:background="@drawable/border_gray"
                        android:textColor="@color/estafeta_text"/>
                </LinearLayout>



            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="bottom"
                android:layout_gravity="bottom"
                android:orientation="vertical">
                <Button
                    android:id                      ="@+id/btn_historia"
                    android:layout_width            ="match_parent"
                    android:layout_height           ="wrap_content"
                    android:layout_gravity          ="bottom|center"
                    android:background              ="@color/zxing_transparent"
                    android:textAllCaps             ="false"
                    android:text                    ="@string/ver_historia"
                    android:paddingTop              ="10dp"
                    android:textSize                ="@dimen/sizeMediumText"
                    android:gravity                 ="bottom|center_horizontal"
                    android:layout_marginBottom     ="25dp"
                    android:textColor               ="@color/estafeta_red" />
            </LinearLayout>



        </LinearLayout>

     </LinearLayout>

    </ScrollView>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="bottom|center_horizontal"
            android:background="@color/white">


            <TextView
                android:id="@+id/footer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/footerSize"
                android:layout_marginLeft="10dp"
                android:text="@string/footer"
                android:textColor="@color/estafeta_soft_gray"
                android:textStyle="bold"
                android:padding="5dp"
                android:gravity="top"
                android:layout_gravity="top|center_horizontal"
                />
        </LinearLayout><!--5-->

        <com.getbase.floatingactionbutton.FloatingActionsMenu
            android:id="@+id/history_more"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            fab:fab_addButtonColorNormal="@color/estafeta_blue"
            fab:fab_addButtonColorPressed="@color/estafeta_blue"
            fab:fab_addButtonSize="normal"
            fab:fab_addButtonPlusIconColor="@color/white"
            fab:fab_expandDirection="up"
            android:layout_gravity="bottom|right"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginEnd="10dp">

            <com.getbase.floatingactionbutton.FloatingActionButton
                android:id="@+id/btn_fav_share"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                fab:fab_colorNormal="@color/white"
                fab:fab_colorPressed="@color/white"
                fab:fab_icon="@drawable/ic_share"
                fab:fab_title="Share"/>

            <com.getbase.floatingactionbutton.FloatingActionButton
                android:id="@+id/btn_fav_call"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                fab:fab_colorNormal="@color/white"
                fab:fab_colorPressed="@color/white"
                fab:fab_icon="@drawable/ic_call"
                fab:fab_title="Call"/>

        </com.getbase.floatingactionbutton.FloatingActionsMenu>
    </FrameLayout>

</FrameLayout>
